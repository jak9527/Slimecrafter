{
	"schemaVersion": 1,
	"id": "slimecrafter",
	"version": "${version}",
	"name": "Slime Crafter",
	"description": "Inspired by Slime Rancher and Resource Chickens. Capture resource slimes, breed them, and generate resources",
	"authors": [
		"PureFyre"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/slimecrafter/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.slimecrafter.Main"
		],
		"client": [
			"com.example.ExampleModClient"
		]
	},
	"mixins": [
		"slimecrafter.mixins.json",
		{
			"config": "slimecrafter.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.15.0",
		"minecraft": "~1.20.2",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}